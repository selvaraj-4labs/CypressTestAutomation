{
  "name": "cypresstestautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run --record --key 040504f5-28f8-4405-928c-ba2ee87bbb1f && npm run merge && npm run mocha-report",
    "merge": "mochawesome-merge cypress/reports/*.json > mochareports/output.json",
    "mocha-report": "marge mochareports/output.json --reportDir ./ --inline",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open  allure-report",
    "cy:clientLogin": "npx cypress run --record --key 040504f5-28f8-4405-928c-ba2ee87bbb1f --reporter mocha-allure-reporter",
    "test:allure": "npm run cy:clientLogin && npm run report:allure"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.8.3",
    "allure-commandline": "^2.13.8",
    "cypress": "^7.6.0",
    "cypress-cucumber-preprocessor": "^4.1.2",
    "cypress-xpath": "^1.6.2",
    "mkdirp": "^1.0.4",
    "mocha": "^9.0.1",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "rimraf": "^3.0.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {}
}
